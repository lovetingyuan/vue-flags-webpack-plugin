{
  "name": "vue-flags-webpack-plugin",
  "version": "1.0.0",
  "description": "remove useless code by setting flags in .vue SFC file",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "npm run lint && npx tape test/*.test.js | tap-min",
    "lint": "npx standard --fix",
    "check": "npx npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lovetingyuan/vue-flags-webpack-plugin.git"
  },
  "keywords": [
    "vue",
    "flags",
    "webpack",
    "plugin",
    "postcss",
    "loader",
    "sfc",
    "remove",
    "code",
    "ignore",
    "optimization",
    "features"
  ],
  "author": "tingyuan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lovetingyuan/vue-flags-webpack-plugin/issues"
  },
  "homepage": "https://github.com/lovetingyuan/vue-flags-webpack-plugin#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "clear-module": "^3.1.0",
    "find-cache-dir": "^2.0.0",
    "loader-utils": "^1.2.0",
    "schema-utils": "^1.0.0",
    "semver": "^6.0.0",
    "watchpack": "^1.6.0"
  },
  "peerDependencies": {
    "webpack": "^4.1.0"
  },
  "devDependencies": {
    "de-indent": "^1.0.2",
    "end-of-stream": "^1.4.1",
    "fs-extra": "^7.0.1",
    "got": "^9.6.0",
    "he": "^1.2.0",
    "husky": "^1.3.1",
    "latest-version": "^5.0.0",
    "memory-fs": "^0.4.1",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "standard": "^12.0.1",
    "tap-min": "^2.0.0",
    "tape": "^4.10.1",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.29.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "standard": {
    "ignore": [
      "dev-app/**",
      "test/webpack-test/**",
      "test/dev.js"
    ]
  }
}
